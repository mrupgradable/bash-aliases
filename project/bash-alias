# top level link for this dir aliases
#
# !! 1. assumes BASH_ALIASES_DIR has already been set !!
# !! 2. assumes this dir is bash-basic (check BASH_PROJECT_ALIASES_DIR) !!

BASH_PROJECT_ALIASES_DIR="${BASH_ALIASES_DIR}/project"

# project bash-alias (idiolect)
alias project.bash-alias="cd ${BASH_PROJECT_ALIASES_DIR}"

# bash aliases project
. ${BASH_PROJECT_ALIASES_DIR}/project-bash-aliases

# ai project
. ${BASH_PROJECT_ALIASES_DIR}/project-ai

# automata project
. ${BASH_PROJECT_ALIASES_DIR}/project-automata

# bot projects
. ${BASH_PROJECT_ALIASES_DIR}/project-bot

# investor allure
. ${BASH_PROJECT_ALIASES_DIR}/project-investor-allure

# profile jumper project
. ${BASH_PROJECT_ALIASES_DIR}/project-profile-jumper

# auto clustify project
. ${BASH_PROJECT_ALIASES_DIR}/project-auto-clustify

